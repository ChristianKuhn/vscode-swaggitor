{
	"name": "swaggitor",
	"description": "Swaggitor - Swagger extension for VSCode",
	"author": "Christian Kuhn <c.kuhn@qn-solutions.de> (http://www.qn-solutions.de)",
	"license": "MIT",
	"version": "1.1.2",
	"publisher": "qnsolutions",
	"repository": {
		"type": "git",
		"url": "https://github.com/ChristianKuhn/vscode-swaggitor.git"
	},
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:yaml",
		"onLanguage:json"
	],
	"main": "./out/src/main",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Swaggitor configuration",
			"properties": {
				"swaggitor.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VSCode and the language server."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"update-vscode": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"vscode": "^1.1.5",
		"vscode-languageclient": "^3.4.2"
	}
}
